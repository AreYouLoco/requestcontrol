{
    "name": "Just Redirect!",
    "description": "Get to the destination url without any page in the middle attacks.",
    "version": "1.1.0",
    "manifest_version": 2,
    "icons": {
        "48": "icon.png",
        "96": "icon@2x.png"
    },

    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking"
    ],

    "options_ui": {
        "page": "options/options.html"
    },

    "background": {
        "scripts": ["options/OptionsManager.js", "main.js"]
    }
}
